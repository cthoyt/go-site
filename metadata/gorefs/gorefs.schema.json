{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://example.com/gorefs.schema.json",
    "title": "JSON Schema for validating gorefs.yaml",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "authors": {
                "type": "string",
                "description": ""
            },
            "id": {
                "type": "string",
                "pattern": "GO_REF:\\d{7}",
                "description": ""
            },
            "is_obsolete": {
                "type":"boolean",
                "description": ""
            },
            "year": {
                "type": "integer",
                "description": ""
            },
            "layout": {
                "type": "string",
                "description": ""
            },
            "title": {
                "type": "string",
                "description": ""
            },
            "comments": {
                "type": "array",
                "items": {
                    "type": "string"
                },
                "description": ""
            }
        }
    }
}
